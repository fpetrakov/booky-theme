{
	"$schema": "vscode://schemas/color-theme",
	"name": "Booky",
	"colors": {
		"editor.background": "#D4CCB9",
		"editor.foreground": "#45363B",

		"editorCursor.foreground": "#45363B",

		"editor.selectionHighlightBackground": "#CFCFCF81",
		"editor.selectionBackground": "#958b83",

		"editorBracketHighlight.foreground1": "#5E5252",
		"editorBracketHighlight.foreground2": "#5E5252",
		"editorBracketHighlight.foreground3": "#5E5252",
		"editorBracketHighlight.foreground4": "#5E5252",
		"editorBracketHighlight.foreground5": "#5E5252",
		"editorBracketHighlight.foreground6": "#5e5252",
		"editorBracketHighlight.unexpectedBracket.foreground": "#BD100D",

		"editorLineNumber.foreground": "#98999C",
		"editorLineNumber.activeForeground": "#5e5252",

		"activityBar.background": "#D4CCB9",
		"activityBar.foreground": "#45363B",

		"sideBar.background": "#D4CCB9",
		"sideBar.foreground": "#45363B",

		"statusBar.background": "#958B83",
		"statusBar.foreground": "#20111A",

		// TODO: 
		"statusBar.debuggingBackground": "#FFBC5D",
		"statusBar.debuggingForeground": "#000",
		"statusBar.noFolderBackground": "#7A3E9D",
		"statusBar.noFolderForeground": "#fff",

		"panel.background": "#F0F0F0",
		"editorGroupHeader.tabsBackground": "#F0F0F0",
		"editor.findMatchBackground": "#FFBC5D",
		"editor.findMatchHighlightBackground": "#FFD86381",

		"list.activeSelectionBackground": "#DDDDDD",
		"list.activeSelectionForeground": "#000000",
		"list.inactiveSelectionBackground": "#E6E6E6",
		"list.focusHighlightForeground": "#007ACC",

		"quickInputList.focusForeground": "#000000",
		"quickInputList.focusBackground": "#DDDDDD",

		"editorSuggestWidget.selectedBackground": "#DDDDDD",
		"editorSuggestWidget.focusHighlightForeground": "#000000",
		"editorSuggestWidget.highlightForeground": "#000000",
		"editorSuggestWidget.selectedForeground": "#000000",

		"terminal.ansiWhite": "#BBBBBB",
		"terminal.ansiBlack": "#000000",
		"terminal.ansiBlue": "#325CC0",
		"terminal.ansiCyan": "#0083B2",
		"terminal.ansiGreen": "#448C27",
		"terminal.ansiMagenta": "#7A3E9D",
		"terminal.ansiRed": "#AA3731",
		"terminal.ansiYellow": "#CB9000",
		"terminal.ansiBrightWhite": "#FFFFFF",
		"terminal.ansiBrightBlack": "#777777",
		"terminal.ansiBrightBlue": "#007ACC",
		"terminal.ansiBrightCyan": "#00AACB",
		"terminal.ansiBrightGreen": "#60CB00",
		"terminal.ansiBrightMagenta": "#E64CE6",
		"terminal.ansiBrightRed": "#F05050",
		"terminal.ansiBrightYellow": "#FFBC5D"
	},
	"tokenColors": [
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#958B83"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string",
				"string.regexp",
				"constant.other.symbol",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#416978"
			}
		},
		{
			"name": "Numbers, Characters",
			"scope": [
				"constant.numeric",
				"constant.character",
				"constant.keyword",
				"constant"
			],
			"settings": {
				"foreground": "#45363B"
			}
		},
		{
			"name": "Global definitions",
			"scope": "entity.name",
			"settings": {
				"foreground": "#96522B"
			}
		},
		{
			"name": "Punctuation",
			"scope": "punctuation",
			"settings": {
				"foreground": "#5e5252"
			}
		},
		{
			"name": "Invalid",
			"scope": "invalid",
			"settings": {
				"foreground": "#BD100D"
			}
		},

		// TODO
		{
			"name": "Extra: Diff Range",
			"scope": [
				"meta.diff.range",
				"meta.diff.index",
				"meta.separator"
			],
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff From",
			"scope": "meta.diff.header.from-file",
			"settings": {
				"foreground": "#434343"
			}
		},
		{
			"name": "Extra: Diff To",
			"scope": "meta.diff.header.to-file",
			"settings": {
				"foreground": "#434343"
			}
		}
	]
}
